<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.eats.mapper.store.StoreReportMapper">
	<select id="selectReviewForReport" resultType="com.eats.store.model.StoreReportDTO">
		select rev_content, rev_score, rev_menu from review where rev_menu = '쇼유우동'
	</select>
</mapper>