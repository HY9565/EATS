<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.eats.mapper.store.StoreMapper">
  
  	<select id="storeLogin" parameterType="String" resultType="String">
  		select 
  		store_pwd
  		from 
  		store_account_test
  		where
  		store_id=#{storeId}  		
  	
  	</select>
  
  
  </mapper>